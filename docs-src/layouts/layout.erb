<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no"> 
    <!-- Use the title from a page's frontmatter if it has one -->
    <title><%= current_page.data.title || "Viking LiveField" %></title>
    <%= stylesheet_link_tag "docs" %>
  </head>
  <body>
    <%= yield %>
  </body>
    <script src="lib/jquery.js"></script>
    <script src="lib/underscore.js"></script>
    <script src="lib/underscore/inflection.js"></script>
    <script src="lib/backbone.js"></script>
    <script src="lib/viking.js"></script>
    <script src="lib/ext/viking/view.js"></script>
    <%= javascript_include_tag "highlight.pack" %>
    <%= javascript_include_tag "docs" %>
</html>
